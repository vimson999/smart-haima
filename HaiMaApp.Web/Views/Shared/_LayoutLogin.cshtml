<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 海马APP后台管理系统</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/signin.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</head>
<body style="background-color:#33B5E5;">
    <div class="container">
        <div style="margin-bottom:30px;">
            <p class="text-center" style="color:#FEF857;font-size:30px;font-family:Microsoft YaHei;">海马APP后台管理系统用户登录平台</p>
        </div>
        <div>
            @RenderBody()
        </div>
        <footer class="navbar-fixed-bottom" style="background-color:#5B5B5B;height:60px;">            
            <div class="container" style="margin-top:20px; text-align:center;">
                <p class="text-muted text-center">&copy; @DateTime.Now.Year - 海马APP后台管理系统</p>
            </div>
        </footer>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
